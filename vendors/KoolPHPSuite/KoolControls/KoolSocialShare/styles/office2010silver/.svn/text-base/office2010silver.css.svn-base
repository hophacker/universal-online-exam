.office2010silverKSS
{
	border-radius: 3px 3px 3px 3px;
    display: inline-block;
    font-family: "Segoe UI",Arial,Helvetica,sans-serif;
    font-size: 12px;
    margin: 0;
    padding: 5px;

	background-color: #E6E9ED;
    border: 1px solid #A4ABB2;
}


.office2010silverKSS ul.kssUL
{
	list-style: none outside none;
    margin: 0;
    padding: 0;	
}


.office2010silverKSS li.kssItem
{
	display: inline-block;
	margin:2px 0px 2px 0px;
    vertical-align: middle;
}

.office2010silverKSS ul.kssVertical li.kssItem
{
	display:block;
}

.office2010silverKSS li.kssLineBreak
{
	display:block;
	margin:0px;
}
.office2010silverKSS ul.kssVertical li.kssLineBreak
{
	margin:4px 0px 4px 0px;
	background-color:#A4ABB2;
	height:1px;
}


.office2010silverKSS a.kssLink
{
	text-decoration: none;
	vertical-align: middle;
}


.office2010silverKSS span.kssText
{
	color: #3B3B3B;
	vertical-align: middle;
	margin:0px 2px 0px 2px;
}

.office2010silverKSS span.kssIcon
{
	background-image: url(SocialShareSprite.png);
    background-repeat: no-repeat;
    display: inline-block;
    height: 16px;
    margin:0px 2px 0px 2px;
    width: 16px;
	vertical-align: middle;
}

/*Facebook*/
.office2010silverKSS .kssFacebook span.kssIcon
{
	background-position: -260px 0;
}
.office2010silverKSS .kssFacebook:hover span.kssIcon
{
	background-position: -260px -20px;
}

