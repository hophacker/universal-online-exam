	font-size:8pt;
}

.hayleftKTS .ktsLI .ktsA
{
	padding-left:0px;
	background:transparent url(t.gif) no-repeat 0 -198px;
}
.hayleftKTS .ktsA .ktsOut
{
	background:none;
	border-right:solid 1px #939286;
	
}

.hayleftKTS .ktsFirst .ktsA
{
	background:transparent url(t.gif) no-repeat 0 -174px;
}


.hayleftKTS .ktsLast .ktsOut
{
	border-bottom:solid 1px #939286;
}

.hayleftKTS .ktsLI .ktsSelected
{
	background:transparent url(t.gif) no-repeat 0 -246px;
}

/*Only read by IE 7-doctype and below*/
*:first-child+html .hayleftKTS .ktsLI .ktsSelected,
/*For IE7-no doctype*/
* html .hayleftKTS .ktsLI .ktsSelected
{
	height:24px;
}

/*Only read by IE 7-doctype and below*/
*:first-child+html .hayleftKTS .ktsLast .ktsSelected,
/*For IE7-no doctype*/
