.web20KSS
{
	border-radius: 3px 3px 3px 3px;
    display: inline-block;
    font-family: "Segoe UI",Arial,Helvetica,sans-serif;
    font-size: 12px;
    margin: 0;
    padding: 5px;

	background-color: #E7F1FF;
    border: 1px solid #BCD2F1;
}


.web20KSS ul.kssUL
{
	list-style: none outside none;
    margin: 0;
    padding: 0;	
}


.web20KSS li.kssItem
{
	display: inline-block;
	margin:2px 0px 2px 0px;
    vertical-align: middle;
}

.web20KSS ul.kssVertical li.kssItem
{
	display:block;
}

.web20KSS li.kssLineBreak
{
	display:block;
	margin:0px;
}
.web20KSS ul.kssVertical li.kssLineBreak
{
	margin:4px 0px 4px 0px;
	background-color:#BCD2F1;
	height:1px;
}


.web20KSS a.kssLink
{
	text-decoration: none;
	vertical-align: middle;
}


.web20KSS span.kssText
{
	color: #3B3B3B;
	vertical-align: middle;
	margin:0px 2px 0px 2px;
}

.web20KSS span.kssIcon
{
	background-image: url(SocialShareSprite.png);
    background-repeat: no-repeat;
    display: inline-block;
    height: 16px;
    margin:0px 2px 0px 2px;
    width: 16px;
	vertical-align: middle;
}

/*Facebook*/
.web20KSS .kssFacebook span.kssIcon
{
	background-position: -260px 0;
}
.web20KSS .kssFacebook:hover span.kssIcon
{
	background-position: -260px -20px;
}

