.forestKSS
{
	border-radius: 3px 3px 3px 3px;
    display: inline-block;
    font-family: "Segoe UI",Arial,Helvetica,sans-serif;
    font-size: 12px;
    margin: 0;
    padding: 5px;

	background-color: #C2D197;
    border: 1px solid #588A4D;
}


.forestKSS ul.kssUL
{
	list-style: none outside none;
    margin: 0;
    padding: 0;	
}


.forestKSS li.kssItem
{
	display: inline-block;
	margin:2px 0px 2px 0px;
    vertical-align: middle;
}

.forestKSS ul.kssVertical li.kssItem
{
	display:block;
}

.forestKSS li.kssLineBreak
{
	display:block;
	margin:0px;
}
.forestKSS ul.kssVertical li.kssLineBreak
{
	margin:4px 0px 4px 0px;
	background-color:#588A4D;
	height:1px;
}


.forestKSS a.kssLink
{
	text-decoration: none;
	vertical-align: middle;
}


.forestKSS span.kssText
{
	color: #3B3B3B;
	vertical-align: middle;
	margin:0px 2px 0px 2px;
}

.forestKSS span.kssIcon
{
	background-image: url(SocialShareSprite.png);
    background-repeat: no-repeat;
    display: inline-block;
    height: 16px;
    margin:0px 2px 0px 2px;
    width: 16px;
	vertical-align: middle;
}

/*Facebook*/
.forestKSS .kssFacebook span.kssIcon
{
	background-position: -260px 0;
}
.forestKSS .kssFacebook:hover span.kssIcon
{
	background-position: -260px -20px;
}

