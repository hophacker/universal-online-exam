.forestKFR,
.forestKFR input,
.forestKFR select

{
	font-family:"Segoe UI",Arial,Helvetica,sans-serif;
	font-size:12px;
	color: #3B3B3B;
}


.kfrRoundCorner
{
    -webkit-border-radius:4px;
    -moz-border-radius:4px;
	border-radius:4px;	
}

/*Headings*/
.forestKFR h1.kfrH1
{
	font-size:16px;
}
.forestKFR h2.kfrH2
{
	font-size:15px;
}
.forestKFR h3.kfrH3
{
	font-size:14px;
}
.forestKFR h4.kfrH4
{
	font-size:13px;
}
.forestKFR h5.kfrH5
{
	font-size:12px;
}
/*FieldSet*/
.forestKFR .kfrFieldSet
{
    background-image: url(FieldsetBgr.png);
    border: 1px solid #588A4D;
	background-repeat:no-repeat;	
}
.forestKFR .kfrFieldSet legend
{
	padding-left:3px;
	padding-right:3px;
	color: #3B3B3B;
}


/*Start:------FieldSet in IE*/
.forestKFR table.kfrRoundedWrapper_FieldSet
{
	display:inline-block;
	_display:inline;
}
.forestKFR table.kfrRoundedWrapper_FieldSet td
{
	vertical-align:middle;
}
.forestKFR table.kfrRoundedWrapper_FieldSet div.kfrRoundedOuter
{
	width:1px;
	font-size:0px;	
	background-color:#588A4D;
}
.forestKFR table.kfrRoundedWrapper_FieldSet div.kfrRoundedInner
{
	width:1px;
	font-size:0px;	
	background-image: url(FieldsetBgr.png);
	border-top:1px solid #588A4D;
	border-bottom:1px solid #588A4D;
}
.forestKFR table.kfrRoundedWrapper_FieldSet .kfrFieldSet
{
	background-position:0px 7px;	
}
/*End:------FieldSet in IE*/

/*Label*/
.forestKFR .kfrLabel
{
	margin-left:2px;
	margin-right:2px;	
}
.forestKFR .kfrDisabled
{
	color:#9F9F9F;
}

/*Validator*/
.forestKFR .kfrValidator
{
	margin-left:5px;
	margin-right:5px;	
	color:red;
}


/*KoolTextBox*/
.forestKFR .kfrTextBox
{
    background-color: #E7F1D7;
    border: 1px solid #618740;
    height: auto;
    line-height: normal;
    padding: 2px 2px 3px;	
}

.forestKFR .kfrTextBox:hover,
.forestKFR .kfrTextBox:focus
{
    border: 1px solid #E7F1D7;
}


.forestKFR .kfrEmpty .kfrTextBox
{
	color:#9F9F9F;
}

.forestKFR .kfrTable
{
	vertical-align: bottom;
    border-collapse: collapse;
    border-width: 0;	
}


.forestKFR .kfrTextBoxWrapper
{
	display:inline-block;
	vertical-align:middle;	
}




/*Start:----------------------------TextBox in IE*/
.forestKFR .kfrRoundedWrapper
{
	border:0px !important;
	display:inline-block;
	vertical-align:middle;
	vertical-align: bottom;
    border-collapse: collapse;
	_display:inline;
}

.forestKFR .kfrRoundedWrapper td
{
	border:0px;
	padding:0px;
	vertical-align:middle;
}


.forestKFR table.kfrRoundedWrapper div.kfrRoundedOuter
{
	width:1px;
	font-size:0px;
	background-color:#618740;
}
.forestKFR table.kfrRoundedWrapper div.kfrRoundedInner
{
	width:1px;
	font-size:0px;
	background-color:#E7F1D7;
	border-top:1px solid #618740;
	border-bottom:1px solid #618740;
	background-image:none;
}

.forestKFR table.kfrRoundedWrapper:hover div.kfrRoundedOuter,
.forestKFR table.kfrFocus div.kfrRoundedOuter
{
	background-color:#E7F1D7 !important;
}
.forestKFR table.kfrRoundedWrapper:hover div.kfrRoundedInner,
.forestKFR table.kfrFocus div.kfrRoundedInner
{
	border-top-color:#E7F1D7 !important;
	border-bottom-color:#E7F1D7 !important;
}
/*End:----------------------------TextBox in IE*/



/*KoolNumericTextBox*/
.forestKFR .kfrTable .kfrSpinUp,
.forestKFR .kfrTable .kfrSpinDown
{
    height: 7px;
    line-height: 7px;
    width: 15px;
    display: block;
    outline: medium none;
    overflow: hidden;
    position: relative;
    text-align: center;
    text-decoration: none;
    text-indent: -2222px;
    z-index: 2;
	cursor:pointer;
}



.forestKFR .kfrTable .kfrSpinUp
{
	background-position: 4px -98px;
	background-image:url(sprite.gif);
}
.forestKFR .kfrTable .kfrSpinUp:hover
{
	background-position: 4px -148px;
}

.forestKFR .kfrTable .kfrSpinDown
{
	margin-top: 3px;
	background-position: 4px -199px;
	background-image:url(sprite.gif);
}
.forestKFR .kfrTable .kfrSpinDown:hover
{
	background-position: 4px -249px;
}

/*KoolPasswordTextBox*/
.forestKFR .kfrIndicator
{
	display:inline-block;
	vertical-align:middle;
	text-align:center;
	margin:0px 10px 0px 10px;
	padding:3px 5px 3px 5px;
	border-radius:4px;
}
.forestKFR .kfrIndicator_Score_0
{
	display:none;
}
.forestKFR .kfrIndicator_Score_1
{
	background:#FF7553;
}
.forestKFR .kfrIndicator_Score_2
{
	background:#FFEDA4;
}
.forestKFR .kfrIndicator_Score_3
{
	background:#93C9FF;
}
.forestKFR .kfrIndicator_Score_4
{
	background:#6CFF92;
}
.forestKFR .kfrIndicator_Score_5
{
	background:#20FF58;
}



/*KoolCheckBox*/
.forestKFR .kfrHide
{
	position:absolute;
	left:-9999px;
}
.forestKFR .kfrCheckBox .kfrSub
{
	display:inline-block;
	width:16px;
	height:16px;
	background-image:url(CheckBoxSprites.gif);
	background-position:-3px -3px;
	vertical-align:middle;
}
.forestKFR .kfrCheckBox .kfrOver
{
	background-position:-3px -203px;
}
.forestKFR .kfrCheckBoxSelected .kfrSub
{
	background-position:-3px -423px;
}
.forestKFR .kfrCheckBoxSelected .kfrOver
{
	background-position:-3px -643px;
}

.forestKFR .kfrCheckBox .kfrDisabled
{
	background-position:-3px -863px;
}
.forestKFR .kfrCheckBoxSelected .kfrDisabled
{
	background-position:-3px -1083px;
}

/*KoolRadioButton*/
.forestKFR .kfrRadio .kfrSub
{
	display:inline-block;
	width:16px;
	height:16px;
	background-image:url(RadioSprites.png);
	background-position:-3px -3px;
	vertical-align:middle;
}
.forestKFR .kfrRadio .kfrOver
{
	background-position:-3px -223px;
}
.forestKFR .kfrRadioSelected .kfrSub
{
	background-position:-3px -443px;
}
.forestKFR .kfrRadioSelected .kfrOver
{
	background-position:-3px -643px;
}

.forestKFR .kfrRadio .kfrDisabled
{
	background-position:-3px -863px;
}
.forestKFR .kfrRadioSelected .kfrDisabled
{
	background-position:-3px -1083px;
}

/*KoolDropDownList*/
.forestKFR .kfrRealDropDown
{
	filter:alpha(opacity=0);
	-moz-opacity:0.0;
	opacity:0.0;
	height:22px;
	position:relative;
}
/*Safari*/
body:nth-of-type(1) .forestKFR .kfrRealDropDown
{
	line-height:18px;
}
.forestKFR .kfrDropDownList
{
	display:inline-block;
	padding-right:1px;
	cursor:default;
	overflow:hidden;
	vertical-align:middle;
}

.forestKFR .kfrOut
{
	display:inline-block;
	position:relative;
	background-image:url(ComboSprites.gif);
	height:22px;
}



.forestKFR .kfrDropDownList .kfrSub
{
	position:absolute;
	float:left;
	left:0px;
	height:22px;
	padding-left:4px;
	overflow:hidden;
	line-height:22px;
}
.forestKFR .kfrDropDownList .kfrArrow
{
	position:absolute;
	display:inline-block;
	float:right;
	height:22px;
	width:22px;
	height:22px;
	background-image:url(ComboSprites.gif);
 	background-position: right -22px;
	right:-1px;
}
.forestKFR .kfrDropDownList:hover .kfrOut
{
 	background-position: 0 -44px;
}

.forestKFR .kfrDropDownList:hover .kfrArrow
{
 	background-position: right -66px;
}

.forestKFR .kfrDropDownList .kfrDisabled
{
	background-position: 0 -176px !important;
}
.forestKFR .kfrDropDownList .kfrDisabled .kfrArrow
{
	background-position: right -198px;
}
.forestKFR .kfrDropDownList .kfrDisabled .kfrSub
{
	color:#9F9F9F;
}



/*KoolButton*/
.forestKFR a.kfrButtonWrapper
{
	background-image: url(ButtonSprites.gif);	
    background-color: transparent;
    background-position: right 0;
    background-repeat: no-repeat;
    border: 0 none;
    display: inline-block;
    height: 22px;
    position: relative;
    text-align: center;
    text-decoration: none;
    white-space: nowrap; 
	vertical-align:middle;
	cursor:pointer;     
}

.forestKFR a.kfrButtonWrapper span.kfrOut 
{
	background-image: url(ButtonSprites.gif);
    background-color: transparent;
    background-position: left -22px;
    background-repeat: no-repeat;
    border: 0 none;
    display: inline-block;
    height: 22px;
    text-align: center;
    text-decoration: none;
    white-space: nowrap;    
	margin-right:4px;
}


.forestKFR a.kfrButtonWrapper input.kfrButton
{
	background:none;
    border: 0 none;
    cursor: pointer;
    display: block;
    height: 22px;
    outline: medium none;
    overflow: visible;
    padding-left: 10px;
    padding-right: 6px;
   	padding-bottom:1px;
    text-align: center;
	vertical-align:middle;
	color: #FFFFFF;
}

.forestKFR a.kfrButtonWrapper:hover
{
	background-position: right -44px;
}
.forestKFR a.kfrButtonWrapper:hover span.kfrOut
{
	background-position: left -66px;
}

.forestKFR a.kfrButtonDown:hover 
{
	background-position: right -88px;
}
.forestKFR a.kfrButtonDown:hover span.kfrOut
{
	background-position: left -110px;
}

.forestKFR a.kfrButtonWrapper span.kfrLeftImage
{
	display:block;
	height:16px;
	width:16px;
	position:absolute;
	top:3px;
	left:5px;
}

.forestKFR a.kfrButtonWrapper span.kfrSpaceLeft .kfrButton
{
	padding-left:22px;
}

.forestKFR a.kfrButtonWrapper span.kfrRightImage
{
	display:block;
	height:16px;
	width:16px;
	position:absolute;
	top:3px;
	right:2px;
}

.forestKFR a.kfrButtonWrapper span.kfrSpaceRight .kfrButton
{
	padding-right:22px;
}
.forestKFR a.kfrButtonWrapper span.kfrSpaceBoth .kfrButton
{
	padding-left:22px;
	padding-right:22px;
}
/*KoolLinkButton*/
.forestKFR span.kfrButton
{
    line-height: 22px;
}
.forestKFR a.kfrButtonWrapper
{
	outline:none;
}
.forestKFR  a.kfrButtonLink
{
	background-image:none;
    border: 1px solid #7A1B05;
    color: #000000;
    text-decoration: none;	
}
.forestKFR a.kfrButtonLink:hover
{
	background-color:#EDB722;
	color:#7A1B05;
}
.forestKFR  a.kfrButtonLink span.kfrOut
{
	background-image:none;	
}
.forestKFR a.kfrButtonLink:active
{
	background-color:#EDB722;
}

/*KoolSplitButton*/
.forestKFR a.kfrSplitButton span.kfrOut
{
	margin-right:18px;
}
.forestKFR a.kfrSplitButton span.kfrSplitRight
{
	background-image: url(ButtonSprites.gif);
    background-position: 0 -137px;
    right: 0;
	top:0;	
	height: 100%;
	width:18px;
	display:block;
	position:absolute;
}
.forestKFR a.kfrSplitButton span.kfrSplitRight:hover
{
    background-position: -18px -137px;
}

.forestKFR a.kfrSplitArrowDown span.kfrSplitRight:hover
{
	background-position: -36px -137px;
}


.forestKFR a.kfrSplitButton div.kfrSplitButtonContainer
{
	position:absolute;
	top:22px;
	left:0px;
}
.forestKFR div.kfrSplitButtonContainer div.kfrContainerInner
{
	border-radius:3px;
	border:solid 1px #32491D;
	background-color:#6E9549;
}



.forestKFR div.kfrSplitButtonContainer div.kfrOutter
{
	margin:1px;
}
.forestKFR div.kfrSplitButtonContainer div.kfrOutter:hover
{
	background-image: url(ButtonSprites.gif);
    background-position: 100% -44px;
	background-repeat:no-repeat;
}

.forestKFR div.kfrSplitButtonContainer div.kfrInner
{
	position:relative;
	text-align:left;
	margin-right:4px;
}
.forestKFR div.kfrOutter:hover div.kfrInner
{
	background-image: url(ButtonSprites.gif);
    background-position: 0 -66px;
	background-repeat:no-repeat;
}

.forestKFR div.kfrSplitButtonContainer span.kfrLeftImage
{
	left:4px;
}


.forestKFR div.kfrInner span.kfrText
{
	color: #EEEEEE;
	background:none;
    border: 0 none;
    line-height: 22px;
    margin-left: 24px;
	padding-right:20px;
}


