	background-position: 0px -150px;
}
.web20KLB .klbButtonAreaLeft .klbTransferAllIn .klbButtonText
{
	background-position: 0 -125px;
}


.web20KLB .klbButtonAreaLeft .klbTransferOutDisabled .klbButtonText
