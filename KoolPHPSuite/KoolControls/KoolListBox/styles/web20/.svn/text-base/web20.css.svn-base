{
	background-position: 0px -200px;
  
}
.web20KLB .klbButtonAreaTop .klbTransferIn .klbButtonText
{
	background-position: 0 -175px;
}
.web20KLB .klbButtonAreaTop .klbTransferAllOut .klbButtonText
{
	background-position: 0px -250px;
}
.web20KLB .klbButtonAreaTop .klbTransferAllIn .klbButtonText
{
	background-position: 0 -225px;
}


.web20KLB .klbButtonAreaTop .klbTransferOutDisabled .klbButtonText
