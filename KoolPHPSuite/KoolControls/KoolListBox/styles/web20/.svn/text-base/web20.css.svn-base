	background-position: -200px -200px;
}
.web20KLB .klbButtonAreaTop .klbTransferInDisabled .klbButtonText
{
	background-position: -200px -175px;
}

.web20KLB .klbButtonAreaTop .klbTransferAllOutDisabled .klbButtonText
{
	background-position: -200px -250px;
}
.web20KLB .klbButtonAreaTop .klbTransferAllInDisabled .klbButtonText
