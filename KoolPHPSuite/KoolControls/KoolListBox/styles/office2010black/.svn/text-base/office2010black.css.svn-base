.office2010blackKLB
{
    display: inline-block;
    position: relative;
    vertical-align: top;	
}


.office2010blackKLB_Scrollable .klbGroup
{
    background: none repeat scroll 0 0 #FFFFFF;
    border: 1px solid #9199A4;
	overflow: auto;
    bottom: 0;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;	
}

.office2010blackKLB .klbList
{
    height: 100%;
    list-style: none outside none;
    margin: 0;
    padding: 0;
    position: relative;
}

.office2010blackKLB .klbItem
{
	line-height: 18px;
    color: #000000;
    font-family: "Segoe UI",Arial,sans-serif;
    font-size: 12px;
    cursor: default;
    padding: 2px 5px;
    white-space: nowrap;	
	-moz-user-select: none; 
	-khtml-user-select: none; 
	-webkit-user-select: none; 
	-o-user-select: none; 		
}


.office2010blackKLB .klbDisabledItem 
{
	color: #AAAAAA;
}

.office2010blackKLB .klbGroup .klbHovered
{
    background: none repeat scroll 0 0 #FFE175;
    color: #000000;
}

.office2010blackKLB .klbGroup .klbSelected {
    background: none repeat scroll 0 0 #F7C840;
    color: #000000;
}

.office2010blackKLB .klbGroup .klbActive {
    border: 1px dotted #555555;
    padding: 1px 4px;
}

.office2010blackKLB .klbItem .klbText,
.office2010blackKLB .klbItem .klbCheck,
.office2010blackKLB .klbItem .klbImage
{
	line-height: 18px;
	vertical-align: middle;
}

.office2010blackKLB .klbItem .klbImage
{
	padding-right:2px;
}

.office2010blackKLB .klbButtonAreaRight
{
	float:right;
	width:30px;
	height:100%;
}

.office2010blackKLB .klbButtonAreaLeft
{
	float:left;
	width:30px;
	height:100%;
}

.office2010blackKLB .klbButtonAreaTop,
.office2010blackKLB .klbButtonAreaBottom
{
	float:left;
	width:100%;
	height:30px;
}
.office2010blackKLB .klbAlignLeft
{
	text-align:left;
}
.office2010blackKLB .klbAlignRight
{
	text-align:right;
}
.office2010blackKLB .klbAlignCenter
{
	text-align:center;
}

.office2010blackKLB .klbAlignTop td
{
	vertical-align:top;
}
.office2010blackKLB .klbAlignBottom td
{
	vertical-align:bottom;
}
.office2010blackKLB .klbAlignMiddle td
{
	vertical-align:middle;
}




.office2010blackKLB .klbButton
{
    cursor: pointer;
    display: block;
    height: 21px;
    line-height: 15px;
    margin: 0 0 5px 6px;
    padding: 0;
    position: relative;
    text-decoration: none;
    width: auto;
}

.office2010blackKLB .klbButtonAreaLeft .klbButton
{
	margin: 0 6px 5px 0;
}
.office2010blackKLB .klbButtonAreaTop .klbButton
{
	display:inline-block;	
	margin: 0 8px 6px 0;
}
.office2010blackKLB .klbButtonAreaBottom .klbButton
{
	display:inline-block;
	margin: 6px 8px 0 0;
}

.office2010blackKLB .klbAlignBottom .klbButton
{
	margin-bottom:2px;
	margin-top:5px;
}
.office2010blackKLB .klbAlignRight .klbButton
{
	margin-left:4px;
	margin-right:4px;
}



.office2010blackKLB .klbButton .klbButtonBL,
.office2010blackKLB .klbButton .klbButtonBR,
.office2010blackKLB .klbButton .klbButtonTR,
.office2010blackKLB .klbButton .klbButtonTL
{
	background-image: url(ButtonNormal.png);
	display: inline-block;
	position: relative;
	width: auto;	
}

.office2010blackKLB .klbButton:hover .klbButtonBL,
.office2010blackKLB .klbButton:hover .klbButtonBR,
.office2010blackKLB .klbButton:hover .klbButtonTR,
.office2010blackKLB .klbButton:hover .klbButtonTL
{
	background-image: url(ButtonHover.png);
}

.office2010blackKLB .klbDisabled:hover .klbButtonBL,
.office2010blackKLB .klbDisabled:hover .klbButtonBR,
.office2010blackKLB .klbDisabled:hover .klbButtonTR,
.office2010blackKLB .klbDisabled:hover .klbButtonTL
{
	background-image: url(ButtonNormal.png);
	cursor:default;
}

.office2010blackKLB .klbButton .klbButtonBL
{
    background-position: 0 100%;
    right: 0;
    top: 4px;	
}

.office2010blackKLB .klbButton .klbButtonBR
{
    background-position: 100% 100%;
    display: inline-block;
    right: -4px;
}
.office2010blackKLB .klbButton .klbButtonTR
{
    background-position: 100% 0;
    bottom: 4px;
    overflow: visible;
    right: 0;	
}
.office2010blackKLB .klbButton .klbButtonTL
{
    background-position: 0 0;
    left: -4px;
}


.office2010blackKLB .klbButton .klbButtonText
{
    background: url(Sprite.png) no-repeat;
    display: inline-block;
	position: relative;
    width: 0px;
    padding-left: 18px;
	height:13px;
	padding-top: 3px;
    text-align: left;
    z-index: 1;
	font-size: 0;
    color: #3B3B3B;
    font-family: "Segoe UI",Arial,sans-serif;	   
}

/*Default and right*/
.office2010blackKLB .klbDelete .klbButtonText
{
   background-position: 0 -50px;
}
.office2010blackKLB .klbMoveUp .klbButtonText
{
  background-position: 0 0;
}
.office2010blackKLB .klbMoveDown .klbButtonText
{
  background-position: 0 -25px;
}

.office2010blackKLB .klbTransferOut .klbButtonText
{
  background-position: 0 -75px;
}
.office2010blackKLB .klbTransferIn .klbButtonText
{
  background-position: 0px -100px;
}

.office2010blackKLB .klbTransferAllOut .klbButtonText
{
  background-position: 0 -125px;
}
.office2010blackKLB .klbTransferAllIn .klbButtonText
{
  background-position: 0px -150px;
}



.office2010blackKLB .klbDeleteDisabled .klbButtonText
{
   background-position: -200px -50px;
}
.office2010blackKLB .klbMoveUpDisabled .klbButtonText
{
  background-position: -200px 0;
}
.office2010blackKLB .klbMoveDownDisabled .klbButtonText
{
  background-position: -200px -25px;
}

.office2010blackKLB .klbTransferOutDisabled .klbButtonText
{
  background-position: -200px -75px;
}
.office2010blackKLB .klbTransferInDisabled .klbButtonText
{
  background-position: -200px -100px;
}

.office2010blackKLB .klbTransferAllOutDisabled .klbButtonText
{
  background-position: -200px -125px;
}
.office2010blackKLB .klbTransferAllInDisabled .klbButtonText
{
  background-position: -200px -150px;
}

