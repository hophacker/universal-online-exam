.inoxKAC *
{
	margin:0px;
	padding:0px;
	white-space:nowrap;
}
.inoxKAC table
{
	border-collapse:collapse;
	width:100%;
}
.inoxKAC .kacUL
{
	margin:0px;
	padding:0px;
}

.inoxKAC .kacLI
{
	list-style-image:none;
	list-style-position:outside;
	list-style-type:none;
}
/*Combobox settings*/
.inoxKAC .kacCombobox
{
	border:solid 1px #7F9DB9;
}

.inoxKAC .kacArrow
{
	border:none;
	width:20px;
	height:20px;
	background:url(arrow.gif) 2px;
}

/*Box*/

.inoxKAC .kacBox
{
	font-size:0pt;
	display:none;
	position:absolute;
	border:solid 1px #989785;
	background:url(bg.gif);
	z-index:9999;
	overflow:hidden;
}

.inoxKAC .kacIframe
{
	display:none;
	position:absolute;
	border:0px;
	filter:alpha(opacity=0);
	-moz-opacity:0.0;
	opacity:0.0;	
	z-index:8888;	
}


.inoxKAC .kacItemBox
{
	overflow:auto;
}

.inoxKAC .kacItem
{
	display:inline-block;
}
/*Hack CSS for Opera*/
html>body .inoxKAC .kacItem
{
	display:block;
}


.inoxKAC .kacOpen
{
	display:block;
}


.inoxKAC .kacA
{
	text-decoration:none;
	cursor:default;
}

.inoxKAC .kacItem .kacIn img
{
	border:0px;
}


.inoxKAC .kacItem .kacIn
{
	color:#333;
	padding-left:5px;
	font-family:Arial;
	font-size:10pt;
	overflow:hidden;
}



.inoxKAC .kacSelectFocus .kacIn
{
	background:url(b.gif);
	height:23px;
	line-height:23px;
	color:#fff;
}

/*Disable Item*/
.inoxKAC .kacDisable .kacIn
{
	color:gray;
}

.inoxKAC .kacDisable a.kacA:hover .kacIn
{
	background:none;
	color:gray;
}

.inoxKAC .kacLoading
{
	background:#EEEEEE;
	text-align:center;
	color:gray;
	font-size:10pt;
	font-family:Arial;
}


.inoxKAC .kacHeader,
.inoxKAC .kacFooter
{
	font-size:10pt;
	font-family:Arial;
	color:Black;
}
.inoxKAC a:focus 
{
	outline:none;
}